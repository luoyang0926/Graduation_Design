<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.xy.blog.mapper.TagMapper">

    <update id="updateTagById">
        update sg_tag set name=#{name} ,remark=#{remark} where del_flag=0 and id=#{id}
    </update>
</mapper>
